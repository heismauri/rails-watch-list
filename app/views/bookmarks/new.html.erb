<div class="container">
  <div class="row justify-content-center">
    <div class="col-6 py-4">
    <h1>New bookmark</h1>
    <%= simple_form_for([@list, @bookmark]) do |f| %>
      <%= f.input :comment %>
      <%= f.association :movie, as: :select %>
      <%= f.button :submit, class: "btn-primary" %>
    <% end %>
    </div>
  </div>
</div>
